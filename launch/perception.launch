<launch>
    <arg name="remote_ip" default="127.0.0.1"/>
    <arg name="venv" value="$(find perception)/yolo_venv/bin/python3"/>
    <node name="perception_node" pkg="perception" type="perception_all_in_one.py" output="screen" launch-prefix = "$(arg venv)" >
        <param name="remote_ip" value="$(arg remote_ip)"/>
    </node>
</launch>